{
    "manifest_version": 3,
    "name": "CSDN Fucker",
    "description": "Allow copying CSDN text and code without logging in.",
    "version": "26.1.1",
    "content_scripts": [
        {
            "matches": [
                "https://*.csdn.net/*"
            ],
            "css": ["style.css"],
            "js": ["script.js"],
            "run_at": "document_start"
        }
    ]
}
